    </com.scnu.zhou.widget.IconTitleCell>

    <com.scnu.zhou.widget.IconTitleCell
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:text="我的发言"
        app:icon="@drawable/icon02"
        app:isdivider="true">

    </com.scnu.zhou.widget.IconTitleCell>

    <com.scnu.zhou.widget.IconTitleCell
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:text="设置"
        app:icon="@drawable/icon03"
        app:isdivider="false">

